        <contentPage id="1143" key="d10343fe-f9ec-4766-b9bd-6f12da60bfce" parentID="1140" level="4" creatorID="0" sortOrder="2" createDate="2017-03-15T15:25:52" updateDate="2017-03-15T15:26:28" nodeName="Scripts" urlName="scripts" path="-1,1129,1130,1140,1143" isDoc="" nodeType="1118" creatorName="Alex Vilmur" writerName="Alex Vilmur" writerID="0" template="1084" nodeTypeAlias="contentPage">
          <content><![CDATA[{
  "name": "1 column layout",
  "sections": [
